{
  "name": "aws-nodejs-typescript",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "start": "set NODE_ENV=\"dev\" & sls offline start -r us-east-1 --stage localdev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "serverless package",
    "bundle": "gulp clean-build && webpack --color",
    "deploy": "serverless deploy",
    "lint": "gulp lint",
    "security:npm-check": "npm-check",
    "preinstall": "npx npm-force-resolutions",
    "package": "set NODE_ENV=\"prod\" && serverless package --stage test --package 1064-EquityAdminSRTools-sls-pkg-test && serverless package --stage dev --package 1064-EquityAdminSRTools-sls-pkg-dev && serverless package --stage stg --package 1064-EquityAdminSRTools-sls-pkg-stg && serverless package --stage prod --package 1064-EquityAdminSRTools-sls-pkg-prod",
    "debug": "node --debug ./node_modules/serverless/bin/serverless offline -s localdev"
  },
  "resolutions": {
    "graceful-fs": "^4.2.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.22",
    "@types/node": "^8.10.66",
    "archiver": "^2.1.1",
    "concat": "^1.0.3",
    "copy-webpack-plugin": "^4.6.0",
    "del": "^3.0.0",
    "gulp": "^4.0.2",
    "gulp-install": "^1.1.0",
    "gulp-run": "^1.7.1",
    "gulp-task-listing": "^1.1.0",
    "gulp-tslint": "^8.1.4",
    "gulp-util": "^3.0.8",
    "gulp-zip": "^3.2.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.20",
    "raw-loader": "^0.5.1",
    "serverless-gulp": "^1.0.10",
    "serverless-offline": "^3.33.0",
    "serverless-plugin-simulate": "0.0.17",
    "serverless-plugin-webpack": "^1.5.1",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^4.4.0",
    "shelljs": "^0.8.4",
    "ts-loader": "^2.3.7",
    "tslint": "^5.20.1",
    "typescript": "^2.9.2",
    "webpack": "^3.12.0"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "dependencies": {
    "@types/jwt-decode": "^3.1.0",
    "aws-sdk": "^2.363.0",
    "cors": "^2.8.5",
    "cryptiles": "^4.1.3",
    "csv-parser": "^3.0.0",
    "csvtojson": "^2.0.10",
    "es-utils": "file:utils",
    "fast-csv": "^4.3.6",
    "gulp": "^4.0.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.12.2",
    "jwt-decode": "^3.1.2",
    "mime": "^2.4.0",
    "mssql": "^4.3.9",
    "request": "^2.88.2",
    "serverless": "^1.83.2"
  }
}
